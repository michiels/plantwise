var new_page_html = $("<%= j render "show" %>")
new_page_html.addClass("forward-page")
var current_page = $(".view-page")
$("#app-viewport").append(new_page_html)

new_page_html.addClass("slide-left")
current_page.addClass("hide-to-left")
$("#navbar-fixed").switchClass("navbar-invisible", "navbar-visible")

setTimeout(1000, function() {
  current_page.remove()
})
